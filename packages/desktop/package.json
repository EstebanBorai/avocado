    "dist:electron": "electron-builder",
    "clean": "mkdir ../../dist && mv ./build/** ../../dist/ && rm -rf ./app ./build"
  },
  "bugs": {
    "url": "https://github.com/whizzes/avocado/issues"
  },
  "dependencies": {
    "app-root-path": "^3.0.0",
    "dotenv": "^8.2.0",
    "electron-better-ipc": "^1.0.1",
    "electron-is-dev": "^1.2.0",
    "jest": "^26.0.1"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/jest": "^25.2.1",
    "electron": "^8.2.5",
    "electron-builder": "^22.6.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "^8.10.1",
    "typescript": "^3.8.3"
  }
}
